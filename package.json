    "@babel/register": "^7.0.0",
    "babel-eslint": "^8.0.1",
    "clean-css": "^4.1.11",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-scss": "^0.3.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^4.3.1"
  },
  "dependencies": {

{
  "name": "videojs-plus",
  "version": "1.5.7",
  "description": "An extension and skin for video.js",
  "main": "dist/videojs-plus.umd.js",
  "module": "dist/videojs-plus.es5.js",
  "typings": "./index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "start": "node server.js",
    "dev": "export NODE_ENV=dev && node server.js & rollup -c -w",
    "build": "rollup -c rollup.config.js",
    "prebuild": "rimraf dist",
    "prepublishOnly": "yarn build",
    "ghpages": "node gh-pages.js"
  },
  "license": "Apache-2.0",
  "peerDependencies": {
    "video.js": ">=6.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/videojs": "^5.16.2",
    "express": "^4.17.1",
    "gh-pages": "^2.0.1",
    "lodash": "^4.17.15",
    "node-sass": "^4.12.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-scss": "^1.0.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "video.js": "^7.6.0",
    "videojs-contrib-quality-levels": "^2.0.9",
    "videojs-plus-font": "Pong420/font#af50d0e"
  }
}
